[user]
 name = Jostein Gogstad
 email = jostein.gogstad@gmail.com
 editor = vim
 signingKey = 27FC9E0CBABCC20A
[alias]
 git = !exec git
[color]
 ui = true
 diff = auto
 status = auto
[color "diff"]
 meta = yellow bold
 frag = magenta bold
 old = red bold
 new = green bold
[color "status"]
 added = cyan bold
 branch = cyan bold
 changed = magenta bold
 deleted = red bold
 untracked = yellow bold
[push]
 default = simple
 editor = vim
 pager = cat
 excludesfile = ~/.gitignore_global
[pull]
 rebase = true
[tag]
 gpgsign = true
[commit]
 gpgsign = false
